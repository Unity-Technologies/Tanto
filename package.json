{
  "name": "tanto",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf static/dist",
    "check": "yarn run lint && yarn run test && yarn run flow && yarn run build",
    "flow": "flow --show-all-errors; test $? -eq 0 -o $? -eq 2",
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "lint": "eslint . --fix",
    "test": "better-npm-run test",
    "test-watch": "better-npm-run test-watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./server.js &",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "HOST": "0.0.0.0"
      }
    },
    "start-dev": {
      "command": "node ./server.js",
      "env": {
        "HOST": "127.0.0.1",
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha-webpack --webpack-config webpack/config.test.js --recursive src/**/__tests__/*.js --full-trace --require source-map-support/register --colors",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "mocha-webpack --webpack-config webpack/config.test.js --recursive src/**/__tests__/*.js --full-trace --require source-map-support/register --colors --watch",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
